{
  "name": "{{armt_appname}}",
  "version": "1.0.0",
  "type": "app",
  
  "subdomain": "armt-{{armt_appname}}",
  "domains": [
    "admin.armt-{{armt_appname}}.jit.su",
    "couch.armt-{{armt_appname}}.jit.su"
  ],
  "scripts": {
    "start": "node node_modules/armt-server/bin/start"
  },
  "armt": {
    "plugins": [
      "hoodie-plugin-appconfig",
      "hoodie-plugin-email",
      "hoodie-plugin-users"
    ]
  }
}
